		function onlyNumbers(evt) {
			var e = event || evt; // for trans-browser compatibility
			var charCode = e.which || e.keyCode;
			if (charCode == 46)
				return true;
			else if (charCode > 31 && (charCode < 48 || charCode > 57 && charCode != 46))
				return false;

			return true;

		}


