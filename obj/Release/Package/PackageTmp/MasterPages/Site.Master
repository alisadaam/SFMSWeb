	<script lang="ja">
		function SucessfulInsert() {
			swal({
				title: "Inserted!",
				text: "Record Inserted Sucessfully.",
				type: "success",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
		function SucessfulUpdate() {
			swal({
				title: "Updated!",
				text: "Record Updated Sucessfully.",
				type: "success",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
		function FinalizeUpdate() {
			swal({
				title: "Finalize!",
				text: "Record Finalize Sucessfully.",
				type: "success",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
		function UnSucessfulInsert() {
			swal({
				title: "Error!",
				text: "Error Inserting Record.",
				type: "error",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
		function SystemError() {
			swal({
				title: "Error!",
				text: "System Error. Please Contact Adminstrator",
				type: "error",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
		function Delete() {
			swal({
				title: "Delete!",
				text: "Record Deleted Sucessfully.",
				type: "error",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
		}
		function ValueExceded() {
			swal({
				title: "Error!",
				text: "Requirement exceeded then Value of BAC Alloc.",
				type: "error",
				confirmButtonColor: "rgb(53, 155, 255)"
			});
			//swal("Inserted!", "Record Inserted Sucessfully.", "success", "rgb(53, 155, 255)");
		}
	</script>
